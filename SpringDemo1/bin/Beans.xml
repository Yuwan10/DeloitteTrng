<?xml version="1.0" encoding="UTF-8"?>

<beans

	xmlns="http://www.springframework.org/schema/beans"

	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"

	xmlns:p="http://www.springframework.org/schema/p"

	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean id="cust" class="com.delloite.cms.model.Customer" autowire="byName" scope="singleton" init-method="d1" destroy-method="d2">
<property name="customerId" value="1"/>
<property name="customerName" value="Bai"/>
<property name="customerAddress" value="Chennai"/>
<property name="billAmount" value="100"/>
</bean>
<bean id="address" class="com.delloite.cms.model.Address">
<property name="AId" value="1222"/>
<property name="city" value="Chennai"/>
<property name="state" value="TamilNadu"/>
</bean>


	</beans>